    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^9.0.1",
    "@tsconfig/svelte": "^3.0.0",
    "@types/file-saver": "^2.0.5",
    "@types/marked": "^4.0.7",
    "@types/qrcode": "^1.5.0",
    "@types/sjcl": "^1.0.30",
    "autoprefixer": "^10.4.12",
    "gh-pages": "^4.0.0",
    "postcss": "^8.4.18",
    "postcss-load-config": "^4.0.1",
    "rollup": "^3.2.0",
    "rollup-plugin-livereload": "^2.0.5",
