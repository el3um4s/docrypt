{
  "name": "docrypt",
  "version": "0.0.6",
  "description": "Encrypt and decrypt documents and images in your browser",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/el3um4s/docrypt.git"
  },
  "bugs": {
    "url": "https://github.com/el3um4s/docrypt/issues"
  },
  "homepage": "https://github.com/el3um4s/docrypt#readme",
  "scripts": {
    "clean": "node clean-build.js",
    "build": "npm run clean && rollup -c",
    "deploy": "node ./gh-pages.js",
    "dev": "rollup -c -w",
    "start": "sirv public",
    "validate": "svelte-check",
    "check-updates": "npx npm-check-updates",
    "check-updates:minor": "npx npm-check-updates --target minor",
    "check-updates:patch": "npx npm-check-updates --target patch"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^14.1.0",
    "@rollup/plugin-typescript": "^8.5.0",
    "@tsconfig/svelte": "^3.0.0",
    "@types/file-saver": "^2.0.5",
    "@types/marked": "^4.0.7",
    "autoprefixer": "^10.4.12",
    "gh-pages": "^4.0.0",
    "postcss": "^8.4.16",
    "postcss-load-config": "^4.0.1",
    "rollup": "^2.79.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-version-injector": "^1.3.3",
    "svelte": "^3.50.1",
    "svelte-check": "^2.9.0",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.1.8",
    "tslib": "^2.4.0",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "blob-polyfill": "^7.0.20220408",
    "file-saver": "^2.0.5",
    "marked": "^4.1.0",
    "sirv-cli": "^2.0.2"
  },
  "type": "module"
}
