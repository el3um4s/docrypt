{
  "name": "docrypt",
  "version": "0.5.4",
  "description": "Encrypt and decrypt documents and images in your browser",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/el3um4s/docrypt.git"
  },
  "bugs": {
    "url": "https://github.com/el3um4s/docrypt/issues"
  },
  "homepage": "https://docrypt.org",
  "scripts": {
    "clean": "node clean-build.js",
    "build": "npm run clean && rollup -c",
