    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "rollup-plugin-version-injector": "^1.3.3",
    "svelte": "^3.51.0",
    "svelte-check": "^2.9.2",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.1.8",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@el3um4s/match": "^0.0.5",
    "@el3um4s/to-try": "^1.0.1",
    "blob-polyfill": "^7.0.20220408",
    "file-saver": "^2.0.5",
    "js-base64": "^3.7.2",
    "marked": "^4.1.1",
    "micro-password-generator": "^0.2.0",
    "qr-scanner": "^1.4.1",
    "qrcode": "^1.5.1",
    "sirv-cli": "^2.0.2",
    "sjcl": "^1.0.8"
